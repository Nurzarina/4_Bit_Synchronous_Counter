#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x557a5dda60f0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x557a5dda6280 .scope module, "counter_4bit_tb" "counter_4bit_tb" 3 1;
 .timescale 0 0;
v0x557a5ddb99a0_0 .var "clk", 0 0;
v0x557a5ddb9a70_0 .net "count", 3 0, v0x557a5ddb97a0_0;  1 drivers
v0x557a5ddb9b40_0 .var "reset", 0 0;
E_0x557a5dd7fe40 .event posedge, v0x557a5dd6bf40_0;
S_0x557a5dd6bcf0 .scope module, "uut" "counter_4bit" 3 8, 4 1 0, S_0x557a5dda6280;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 4 "count";
v0x557a5dd6bf40_0 .net "clk", 0 0, v0x557a5ddb99a0_0;  1 drivers
v0x557a5ddb97a0_0 .var "count", 3 0;
v0x557a5ddb9880_0 .net "reset", 0 0, v0x557a5ddb9b40_0;  1 drivers
E_0x557a5dd6c870 .event posedge, v0x557a5ddb9880_0, v0x557a5dd6bf40_0;
    .scope S_0x557a5dd6bcf0;
T_0 ;
    %wait E_0x557a5dd6c870;
    %load/vec4 v0x557a5ddb9880_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x557a5ddb97a0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x557a5ddb97a0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x557a5ddb97a0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x557a5dda6280;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x557a5ddb99a0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x557a5ddb99a0_0, 0, 1;
    %delay 5, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x557a5dda6280;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x557a5ddb9b40_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x557a5ddb9b40_0, 0, 1;
    %pushi/vec4 20, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x557a5dd7fe40;
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %vpi_call/w 3 32 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x557a5dda6280;
T_3 ;
    %vpi_call/w 3 38 "$monitor", "Time: %0d, count: %b", $time, v0x557a5ddb9a70_0 {0 0 0};
    %vpi_call/w 3 39 "$dumpfile", "counter_4bit_tb.vcd" {0 0 0};
    %vpi_call/w 3 40 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x557a5dda6280 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "counter_4bit_tb.v";
    "counter_4bit.v";
