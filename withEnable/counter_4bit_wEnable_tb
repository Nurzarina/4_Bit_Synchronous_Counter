#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x55cdb5cb8360 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x55cdb5cb84f0 .scope module, "counter_4bit_tb" "counter_4bit_tb" 3 3;
 .timescale -9 -12;
v0x55cdb5ccc9c0_0 .var "clk", 0 0;
v0x55cdb5ccca80_0 .net "count", 3 0, v0x55cdb5ccc6a0_0;  1 drivers
v0x55cdb5cccb50_0 .var "enable", 0 0;
v0x55cdb5cccc50_0 .var "reset", 0 0;
S_0x55cdb5cb8680 .scope module, "uut" "counter_4bit" 3 11, 4 1 0, S_0x55cdb5cb84f0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "enable";
    .port_info 3 /OUTPUT 4 "count";
v0x55cdb5c7fba0_0 .net "clk", 0 0, v0x55cdb5ccc9c0_0;  1 drivers
v0x55cdb5ccc6a0_0 .var "count", 3 0;
v0x55cdb5ccc780_0 .net "enable", 0 0, v0x55cdb5cccb50_0;  1 drivers
v0x55cdb5ccc850_0 .net "reset", 0 0, v0x55cdb5cccc50_0;  1 drivers
E_0x55cdb5cb70e0 .event posedge, v0x55cdb5ccc850_0, v0x55cdb5c7fba0_0;
    .scope S_0x55cdb5cb8680;
T_0 ;
    %wait E_0x55cdb5cb70e0;
    %load/vec4 v0x55cdb5ccc850_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55cdb5ccc6a0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55cdb5ccc780_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x55cdb5ccc6a0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55cdb5ccc6a0_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55cdb5cb84f0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cdb5ccc9c0_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cdb5ccc9c0_0, 0, 1;
    %delay 5000, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55cdb5cb84f0;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cdb5cccc50_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cdb5cccb50_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cdb5cccc50_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cdb5cccb50_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cdb5cccb50_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cdb5cccc50_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cdb5cccc50_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cdb5cccb50_0, 0, 1;
    %delay 200000, 0;
    %vpi_call/w 3 41 "$stop" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55cdb5cb84f0;
T_3 ;
    %vpi_call/w 3 46 "$monitor", "clk : %b, count: %b, reset: %b, enable = %b", v0x55cdb5ccc9c0_0, v0x55cdb5ccca80_0, v0x55cdb5cccc50_0, v0x55cdb5cccb50_0 {0 0 0};
    %vpi_call/w 3 47 "$dumpfile", "counter_4bit_tb.vcd" {0 0 0};
    %vpi_call/w 3 48 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55cdb5cb84f0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "counter_4bit_tb.v";
    "counter_4bit.v";
